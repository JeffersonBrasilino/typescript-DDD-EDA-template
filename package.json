{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "first-install": "yarn install && husky install",
    "start": "nest start",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "check": "npx npm-check -s -u",
    "test": "export NODE_ENV=test && ts-node-dev -r tsconfig-paths/register --transpile-only --clear --ignore-watch node_modules node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:unit": "npm run test -- --config ./jest-unit.config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:watch": "npm run test -- --watch --config ./jest-unit.config.js",
    "test:cov": "npm run test -- --coverage --config ./jest-unit.config.js",
    "test:e2e": "npm run test -- --config ./jest-e2e.config.js",
    "start:dev": "export NODE_ENV=development && ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "start:debug": "export NODE_ENV=development && ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9229 --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "eslint --fix",
    "prepare": "husky install",
    "release": "standard-version",
    "changelog": "standard-version && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:minor": "standard-version --release-as minor && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:major": "standard-version --release-as major && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:patch": "standard-version --release-as patch && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:alpha": "standard-version --prerelease alpha && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/terminus": "^9.1.1",
    "amqp-connection-manager": "^4.1.9",
    "amqplib": "^0.10.3",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dd-trace": "^3.2.0",
    "helmet": "^6.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "^18.0.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^6.5.0",
    "mysql2": "^2.3.3",
    "nest-keycloak-connect": "^1.9.0",
    "nestjs-slack": "^2.0.0",
    "npm": "^8.17.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest-mock/express": "^2.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.3",
    "@types/jest": "28.1.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "auto-changelog": "^2.4.0",
    "commitlint": "^17.1.2",
    "coveralls": "^3.1.1",
    "dotenv": "16.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.1",
    "faker": "5.5.3",
    "faker-br": "^0.4.1",
    "git-commit-msg-linter": "^4.2.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "jest-mock-extended": "^2.0.7",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "commit": true
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
